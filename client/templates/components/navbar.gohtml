                <div class="dropdown-actions">
                    <a href="/feed?category=pc">
                        <button value="PC" class="dropdown-options">PC</button>
                    </a>
                </div>
                <div class="dropdown-actions">
                    <a href="/feed?category=xbox">
                        <button value="Xbox" class="dropdown-options">Xbox</button>
                    </a>
                </div>
                <div class="dropdown-actions">
                    <a href="/feed?category=playstation">
                        <button value="Playstation" class="dropdown-options">Playstation</button>
                    </a>
                </div>
                <div class="dropdown-actions">
                    <a href="/feed?category=nintendo">
                        <button value="Nintendo" class="dropdown-options">Nintendo</button>
                    </a>
                </div>
                <div class="dropdown-actions">
                    <a href="/feed?category=mobile">
                        <button value="Nintendo" class="dropdown-options">Mobile</button>
                    </a>
                </div>
                <div class="dropdown-actions">
                    <a href="/feed?category=others">
                        <button value="Accessories" class="dropdown-options">Others</button>
                    </a>
                </div>
            </div>
        </div>
        <div class="search-bar">
            <form class="search-form" method="post">
                <label for="search">
                    <input type="search" name="search" spellcheck="false" placeholder=" Search..."
                            {{/*                               value="{{.Search}}"*/}}
                           id="search-input">
                    <button type="submit" name="submit" class="search-btn"><i class="fa fa-search"></i></button>
                </label>
            </form>
        </div>
        <div class="account-options">
            {{if not .ConnectedUser}}
                <div class="actions">
                    <a href="/login">
                        <button value="Login" class="log-in">Login</button>
                    </a>
                </div>
                <div class="actions">
                    <a href="/sign-up">
                        <button value="Sign-up" class="sign-up">Sign Up</button>
                    </a>
                </div>
            {{else}}
                <div class="actions username">
                    <a>
                        <button value="Login" class="log-in username_dropdown">{{.ConnectedUser.Username}}</button>
                        {{if eq .ConnectedUser.IsOnline true}}
                            <div class="circle connected">
                                {{if eq .ConnectedUser.ProfilePic ""}}
                                    <img class="avatar"
                                         src=" data:image/png;base64,"
                                         alt="test">
                                {{else}}
                                    <img class="avatar" src="{{safeURL .ConnectedUser.ProfilePic}}" alt="test">
                                {{end}}
                            </div>
                        {{else}}
                            <div class="circle disconnected">
                                {{if eq .ConnectedUser.ProfilePic ""}}
                                    <img class="avatar"
                                         src=" data:image/png;base64,"
                                         alt="test">
                                {{else}}
                                    <img class="avatar" src="{{safeURL .ConnectedUser.ProfilePic}}" alt="test">
                                {{end}}
                            </div>
                        {{end}}
                    </a>
                    <div class="dropdown none">
                        <div class="dropdown-actions">
                            <a href="/">
                                <button value="Edit-username" class="dropdown-options">Profile</button>
                            </a>
                        </div>
                        <div class="dropdown-actions">
                            <a href="/">
                                <button value="Edit-password" class="dropdown-options">My posts</button>
                            </a>
                        </div>
                        <div class="dropdown-actions">
                            <a href="/">
                                <button value="Edit-password" class="dropdown-options">My answers</button>
                            </a>
                        </div>
                        <div class="dropdown-actions">
                            <a href="/settings">
                                <button value="Edit-password" class="dropdown-options">Settings</button>
                            </a>
                        </div>
                        <div class="dropdown-actions">
                            <a href="/logout">
                                <button value="Edit-password" class="dropdown-options log-out">Log out</button>
                            </a>
                        </div>
                    </div>
                </div>
            {{end}}
        </div>
    </div>
{{end}}